{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Effect": "Allow",
      "Action": [
        "logs:CreateLogGroup",
        "logs:CreateLogStream",
        "logs:PutLogEvents"
      ],
      "Resource": [
        "arn:aws:logs:us-east-1:861229788715:log-group:/aws/lambda/query-api-predev",
        "arn:aws:logs:us-east-1:861229788715:log-group:/aws/lambda/query-api-predev:*:*"
      ]
    },
    {
      "Effect": "Allow",
      "Action": [
        "secretsmanager:DescribeSecret",
        "secretsmanager:GetSecretValue"
      ],
      "Resource": [
        "arn:aws:secretsmanager:us-east-1:861229788715:secret:query/predev/*",
        "arn:aws:secretsmanager:us-east-1:861229788715:secret:dcp/query/predev/database-*"
      ]
    },
    {
      "Effect": "Allow",
      "Action": [
        "sqs:SendMessage"
      ],
      "Resource": [
        "arn:aws:secretsmanager:us-east-1:861229788715::dcp-query-data-input-queue-predev"
      ]
    }
  ]
}
